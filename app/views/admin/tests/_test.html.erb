<tr>
  <td><%= test.title %></td>
  <td><%= test_level(test) %></td>
  <td><%= test.category.title %></td>
  <% if test.author.present? %>
    <td><%= test.author.email %></td>
  <% else %>
    <td>No author</td>
  <% end %>
  <td><%= test.questions.size %></td>
  <td><%= link_to "Show", admin_test_path(test) %></td>
  <td><%= link_to "Edit", edit_admin_test_path(test) %></td>
  <td>
    <%= link_to "Delete", admin_test_path(test), method: :delete, data: { confirm: "Are you sure?" } %>
  </td>
</tr>
